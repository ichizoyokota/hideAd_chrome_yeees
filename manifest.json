{
  "name": "hideAd_chrome_yeees",
  "version": "0.4",
  "manifest_version": 3,
  "description": "Hide Advertising Chrome Extension",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "youtube.js"
      ],
      "css": [
        "adHide.css"
      ]
    },
    {
      "matches": [
        "https://www.yahoo.co.jp/*",
        "https://news.yahoo.co.jp/*"
      ],
      "js": [
        "youtube.js"
      ],
      "css": [
        "adHide.css"
      ]
    }
  ],
  "homepage_url": "https://www.yeees.in"
}
